{
  "version": "1.0.0",
  "id": "@shopgate/magento-favorites",
  "configuration": {
    "magentoUrl" : {
      "type": "bigApi",
      "destination" : "backend",
      "default": null,
      "params": {
        "method":  "GET",
        "service": "config",
        "path": "/v1/shop/%(shopId)s/checkout_web_checkout_shopify?parsed=true",
        "key": "value.backend.magentoUrl"
      }
    },
    "allowSelfSignedCertificate" : {
      "type": "bigApi",
      "destination" : "backend",
      "default": false,
      "params": {
        "method":  "GET",
        "service": "config",
        "path": "/v1/shop/%(shopId)s/checkout_web_checkout_shopify?parsed=true",
        "key": "value.backend.allowSelfSignedCertificate"
      }
    }
  }
}
